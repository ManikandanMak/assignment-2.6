import java.util.*;
import java.lang.*;
import java.io.*;
class acad
{
public static void main (String[] args) throws java.lang.Exception
{
Scanner sc = new Scanner(System.in);
int x=sc.nextInt();
int y=sc.nextInt();
sum(x);
sum(x,y);
} 
static void sum(int x)
{
System.out.println(x+100);
}
static void sum(int x,int y)
{
System.out.println(x+y);
}
}
