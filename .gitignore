import java.util.*;
import java.lang.*;
import java.io.*;
class acad
{
public static void main (String[] args) throws java.lang.Exception
{
Scanner sc = new Scanner(System.in);
int	x=sc.nextInt();
int	y=sc.nextInt();
for(int i=x;i<=y;i++)
{
if(i%2==0)
{
System.out.println("even"+i);
}
else
{
System.out.println("odd"+i);
}
}
} 
}


